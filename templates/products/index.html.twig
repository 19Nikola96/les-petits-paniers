{% extends 'base.html.twig' %}

{% block title %}Hello ProductsController!{% endblock %}

{% block body %}


    <div class="container">
        <h1>Nos paniers</h1>
        {% for product in products %}
            <strong>{{ product.label }}</strong><br>
            {{ product.description }}<br>
            {{ product.price }}<br>
        {% endfor %}
    </div>
{% endblock %}
