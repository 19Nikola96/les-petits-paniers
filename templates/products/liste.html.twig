{% extends 'base.html.twig' %}

{% block title %}Hello IndexController!{% endblock %}



{% block body %}
    <h1>Liste des paniers</h1>
    {% for product in list %}

        <div class="list">
            <li><strong>{{ product.label }}</li></strong> <hr>
            <li>{{ product.photo }}</li>
            <li>{{ product.description }}</li>
            <li>{{ product.price }}</li>
        </div>
    {% endfor %}


{% endblock %}